{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "architraigupta/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "server",
			"image": "architraigupta/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "architraigupta/multi-worker",
			"hostname": "worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "architraigupta/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}

			],
			"links": [
				"client",
				"server"
			]

		}

	]
}